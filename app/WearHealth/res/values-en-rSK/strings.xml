    <string name="label_active_fast_sick_any">Pokiaľ ste chorý, intenzita aktivity by mala byť nižšia, prípadne zvážte úplné prerušenie aktivity.</string>
    <string name="label_active_fast_tired_any">Mierne cvičenie pomáha zmierniť psychickú únavu. Pokiaľ sa cítite psychicky unavený, oddýchnite si skôr, ako začnete aktivitu.</string>
    <string name="label_active_fast_well_bad">Pre uvoľnenie negatívnych emócií, vyberte pokojné aktivity so zábavnými prvkami, aby ste dosiahli najlepšie výsledky.</string>
    <string name="label_active_fast_well_good">Neurologické a svalové funkcie budú v rovnováhe, pokiaľ budete v dobrej nálade. Tá pozitívne ovplyvní vašu aktivitu.</string>
    <string name="label_active_fast_well_medium">Krátke obdobie vysokej intenzity cvičenia môže podporovať športový výkon a rýchlosť. Zvýšený výkon nie je vhodné vykonávať dlhodobo a je potrebná zvýšená opatrnosť na prevenciu zranení.</string>
    <string name="label_active_normal">Aerobik</string>
    <string name="label_active_normal_sick_any">Pokiaľ ste chorý, intenzita aktivity by mala byť nižšia, prípadne zvážte úplné prerušenie aktivity.</string>
    <string name="label_active_normal_tired_any">Mierne cvičenie pomáha zmierniť psychickú únavu. Pokiaľ sa cítite psychicky unavený, oddýchnite si skôr, ako začnete aktivitu.</string>
    <string name="label_active_normal_well_bad">Mierne cvičenie môže znižovať negatívne emócie a zbavuje stresu.</string>
    <string name="label_active_normal_well_good">Neurologické a svalové funkcie budú v rovnováhe, pokiaľ budete v dobrej nálade. Tá pozitívne ovplyvní vašu aktivitu.</string>
    <string name="label_active_normal_well_medium">Toto je počas aktivity stredne vysokej intenzity. Pomáha efektívne spaľovať tuk a podporuje funkcie srdca a pľúc. Je cieľovou aktivitou pre väčšinu ľudí.</string>
    <string name="label_active_slow">Spaľovanie tuku</string>
    <string name="label_active_slow_sick_any">Pokiaľ ste chorý, intenzita aktivity by mala byť nižšia, prípadne zvážte úplné prerušenie aktivity.</string>
    <string name="label_active_slow_tired_any">Mierne cvičenie pomáha zmierniť psychickú únavu. Pokiaľ sa cítite psychicky unavený, oddýchnite si skôr, ako začnete aktivitu.</string>
    <string name="label_active_slow_well_bad">Mierne cvičenie môže znižovať negatívne emócie a zbavuje stresu.</string>
